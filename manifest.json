{
  "manifest_version": 3,
  "name": "Orbi",
  "description": "An extension to display the color palatte of the active tab",
  "version": "1.0",
  "background": {
    "service_worker": "background.js"
  },
  "action": {
    "default_icon": "images/icon-11.png"
  },
  "permissions": ["scripting", "tabs", "activeTab"]
}
